  "devDependencies": {
    "@qwik.dev/core": "2.0.0-beta.13",
    "@qwik.dev/router": "2.0.0-beta.13",
    "@types/node": "^20.8.4",
    "ts-morph": "^23.0.0",
    "typescript": "5.4.5",
    "vite": "7.1.11",
    "vite-tsconfig-paths": "^4.2.1"
  }
